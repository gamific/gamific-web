
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<script type="text/javascript">
    $("#Hierarchy").addClass("active");
</script>

<div>
    <div class="pageheader">
        <h2><i class="fa fa-tachometer"></i> Resultados</h2>
    </div>
    <input hidden type="text" value="@ViewBag.EpisodeId" id="EpisodeId" />
    <div class="main">
        <div class="chartTree" id="custom-colored"> </div>
    </div>

    <script type="text/javascript">
        $(document).ready(function () {
            searchHierarchy($('#EpisodeId').val());
        });
    </script>

</div>


<script src="/Content/Js/app/hierarchy.js" charset="iso-8859-1"></script>


